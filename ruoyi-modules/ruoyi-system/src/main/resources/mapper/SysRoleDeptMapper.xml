<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.system.infrastructure.role.repository.mapper.SysRoleDeptMapper">

    <resultMap id="BaseResultMap" type="com.ruoyi.system.infrastructure.role.repository.po.SysRoleDeptPo">
            <id property="roleId" column="role_id" jdbcType="BIGINT"/>
            <id property="deptId" column="dept_id" jdbcType="BIGINT"/>
    </resultMap>

    <sql id="Base_Column_List">
        role_id,dept_id
    </sql>
    <insert id="insertBatch">
        INSERT INTO sys_role_dept (role_id, dept_id)
        VALUES
        <foreach collection="list" item="item" separator=",">
            (#{item.roleId}, #{item.deptId})
        </foreach>
    </insert>
</mapper>
